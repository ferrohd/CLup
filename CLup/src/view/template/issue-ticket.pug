doctype html
html(lang="en")
    head
        - function Store(name, vat, lat, lng, capacity, distance, inline, address) { this.name = name; this.vat = vat; this.lat = lat; this.lng = lng; this.capacity = capacity; this.distance = distance; this.inline = inline; this.address = address }
        - var store = new Store("Store name 1", "1", "lat", "lng", 20, 0.5, 15, "Via Benedetto Spinoza, 4, 20131 Milano MI");
        - function Ticket(id, date, user, store, inside, qrcode) { this.id = id; this.date = date; this.user = user; this.store = store; this.inside = inside; this.qrcode = qrcode}
        - var ticket = new Ticket("1", "23-12-2020 - 10:21", "user", "store", "inside", "");

        include include/head.pug
        title Print Ticket - Customers LineUp
    body.alt
        h1 Here, the ticket for your customer!
        div.btn-floating-container
            a#close-btn.btn-floating.btn-large.waves-effect.waves-dark
                i.material-icons close
        
        img.qr-code(src="data:image/png;base64," + ticket.qrcode)
        h3.qr-code-description Show this QR code to the store manager at the entrance

        div.info-container
            i.material-icons store
            div
                h2
                    b #{store.name}
                h2 #{store.address}

        div.info-container
            i.material-icons watch_later
            div
                h2 Issued on
                h2
                    b #{ticket.date}
        
        form#form.hidden-form(method="POST" action="/ticket/delete")
            input(name="ticket" type="hidden" value=ticket.id)
        
        a#print-btn.waves-effect.waves-light.btn.primary Print the Ticket
        button#form-btn.waves-effect.waves-light.btn.secondary(type="submit" form="form") Delete the Ticket

        include include/script.pug
        script(type="text/javascript" src="js/issue-ticket.js")